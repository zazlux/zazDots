#!/usr/bin/env bash

curl --silent http://lxer.com/ > ~/temp/lxer/raw-lxer-src.txt && \

pup 'span.storyheadline text{}' <  ~/temp/lxer/raw-lxer-src.txt > $HOME/temp/lxer/endResult.txt && \

while read -r line
do
    pv -L 14 -q <<< "$line"
    sleep 0.7
    printf '\n'
done < $HOME/temp/lxer/endResult.txt

